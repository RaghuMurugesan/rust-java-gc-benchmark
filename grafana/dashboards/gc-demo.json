{
  "uid": "gc-demo",
  "title": "GC Demo - G1GC vs ZGC vs Rust",
  "timezone": "browser",
  "refresh": "2s",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "P99 Latency Comparison",
      "type": "timeseries",
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"java-g1gc\"}[10s]))",
          "legendFormat": "G1GC P99",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"java-zgc\"}[10s]))",
          "legendFormat": "ZGC P99",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"rust\"}[10s]))",
          "legendFormat": "Rust P99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "lineWidth": 2
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "G1GC P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "ZGC P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Rust P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 2,
      "title": "G1GC Latency Breakdown",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 10 },
      "targets": [
        { "expr": "histogram_quantile(0.5, rate(http_request_duration_seconds_bucket{job=\"java-g1gc\"}[10s]))", "legendFormat": "P50", "refId": "A" },
        { "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"java-g1gc\"}[10s]))", "legendFormat": "P99", "refId": "B" },
        { "expr": "histogram_quantile(0.999, rate(http_request_duration_seconds_bucket{job=\"java-g1gc\"}[10s]))", "legendFormat": "P999", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "P50" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P999" }, "properties": [{ "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 3,
      "title": "ZGC Latency Breakdown",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 10 },
      "targets": [
        { "expr": "histogram_quantile(0.5, rate(http_request_duration_seconds_bucket{job=\"java-zgc\"}[10s]))", "legendFormat": "P50", "refId": "A" },
        { "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"java-zgc\"}[10s]))", "legendFormat": "P99", "refId": "B" },
        { "expr": "histogram_quantile(0.999, rate(http_request_duration_seconds_bucket{job=\"java-zgc\"}[10s]))", "legendFormat": "P999", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "P50" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P999" }, "properties": [{ "id": "color", "value": { "fixedColor": "dark-orange", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 4,
      "title": "Rust Latency Breakdown",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 10 },
      "targets": [
        { "expr": "histogram_quantile(0.5, rate(http_request_duration_seconds_bucket{job=\"rust\"}[10s]))", "legendFormat": "P50", "refId": "A" },
        { "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"rust\"}[10s]))", "legendFormat": "P99", "refId": "B" },
        { "expr": "histogram_quantile(0.999, rate(http_request_duration_seconds_bucket{job=\"rust\"}[10s]))", "legendFormat": "P999", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "P50" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P99" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "P999" }, "properties": [{ "id": "color", "value": { "fixedColor": "dark-green", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 5,
      "title": "Request Throughput (req/sec)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 18 },
      "targets": [
        { "expr": "rate(http_request_duration_seconds_count{job=\"java-g1gc\"}[10s])", "legendFormat": "G1GC", "refId": "A" },
        { "expr": "rate(http_request_duration_seconds_count{job=\"java-zgc\"}[10s])", "legendFormat": "ZGC", "refId": "B" },
        { "expr": "rate(http_request_duration_seconds_count{job=\"rust\"}[10s])", "legendFormat": "Rust", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "G1GC" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "ZGC" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Rust" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 6,
      "title": "Memory RSS",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 26 },
      "targets": [
        { "expr": "container_memory_rss{name=~\".*java-g1gc.*\"}", "legendFormat": "G1GC", "refId": "A" },
        { "expr": "container_memory_rss{name=~\".*java-zgc.*\"}", "legendFormat": "ZGC", "refId": "B" },
        { "expr": "container_memory_rss{name=~\".*rust.*\"}", "legendFormat": "Rust", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "G1GC" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "ZGC" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Rust" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      }
    },
    {
      "id": 7,
      "title": "CPU Usage (%)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 26 },
      "targets": [
        { "expr": "rate(container_cpu_usage_seconds_total{name=~\".*java-g1gc.*\"}[10s]) * 100", "legendFormat": "G1GC", "refId": "A" },
        { "expr": "rate(container_cpu_usage_seconds_total{name=~\".*java-zgc.*\"}[10s]) * 100", "legendFormat": "ZGC", "refId": "B" },
        { "expr": "rate(container_cpu_usage_seconds_total{name=~\".*rust.*\"}[10s]) * 100", "legendFormat": "Rust", "refId": "C" }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "max": 100 },
        "overrides": [
          { "matcher": { "id": "byName", "options": "G1GC" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "ZGC" }, "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "Rust" }, "properties": [{ "id": "color", "value": { "fixedColor": "green", "mode": "fixed" } }] }
        ]
      }
    }
  ],
  "schemaVersion": 39
}
